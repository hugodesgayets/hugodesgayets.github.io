<script src="static/js/jquery-1.11.3.min.js"></script>
<script src="static/js/main.js"></script>

<script>

var rotate = function() {
  $("#ios")
    .delay(5000).queue(function() {
        $(this).css({
            "background-position-y": "0%"
        });
        $(this).dequeue();
    })
    .delay(5000).queue(function() {
        $(this).css({
            "background-position-y": "50%"
        });
        $(this).dequeue();
    })
    .delay(5000).queue(function(next) {
        $(this).css({
            "background-position-y": "100%"
        });
        $(this).dequeue();
        next();
    })

    .queue(rotate);
};

rotate();

var rotateAndroid = function() {
  $("#android")
    .delay(5000).queue(function() {
        $(this).css({
            "background-position-y": "0%"
        });
        $(this).dequeue();
    })
    .delay(5000).queue(function() {
        $(this).css({
            "background-position-y": "50%"
        });
        $(this).dequeue();
    })
    .delay(5000).queue(function(next) {
        $(this).css({
            "background-position-y": "100%"
        });
        $(this).dequeue();
        next();
    })
    .queue(rotateAndroid);
};

rotateAndroid();


</script>

